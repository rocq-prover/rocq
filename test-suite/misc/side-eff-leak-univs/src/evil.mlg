DECLARE PLUGIN "rocq-runtime.plugins.evil"

{
open Ltac_plugin
open Stdarg
}

TACTIC EXTEND magic
| [ "magic" ident(i) ident(j) ] -> {
  let open Constrexpr in
  DeclareUniv.do_constraint ~poly:false [ UnivCst ([(CType (Libnames.qualid_of_ident i), 0)], (Univ.UnivConstraint.Le, true), [(CType (Libnames.qualid_of_ident j), 0)])]; Proofview.tclUNIT()
}
END
